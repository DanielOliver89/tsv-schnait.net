#An dieser Stelle wird das bisherige Menü über Fluid gerendert, da das Menü mit afx nicht 1:1 nachgebaut werden konnte.
prototype(tsv.schnait:MainMenu) < prototype(Neos.Fusion:Template) {
    templatePath = 'resource://tsv.schnait/Private/Templates/FusionObjects/MainMenu.html'
    items = Neos.Neos:MenuItems {
        startingPoint = 0
        maximumLevels = 3
    }
    sectionName = 'itemList'
    #items = ${props.menuItems}
}
